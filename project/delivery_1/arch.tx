Model:
    'architecture' ':'
        elements*=Element
;

Element:
    Component | Connector
;

Component:
    'component' type=ComponentType name=ID
;

Connector:
    'connector' type=ConnectorType from=[Component] '->' to=[Component]
;

ComponentType:
    'engine' | 'matcher' | 'frontend' | 'loadbalancer' | 'pub_sub'
;


ConnectorType:
    'http' | 'pub_sub_driver' | 'web_socket'
;
