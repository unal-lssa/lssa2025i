Model:
    'architecture' ':'
        elements*=Element
;
Element:
    Component | Connector
;
Component:
    'component' type=ComponentType name=ID
;
Connector:
    'connector' type=ConnectorType from=[Component] '->' to=[Component]
;
ComponentType:
    'frontend' | 'loadbalancer' | 'backend' | 'database'  
;
ConnectorType:
    'http' | 'db_connector'
;