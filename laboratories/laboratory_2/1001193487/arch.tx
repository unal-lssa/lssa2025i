Model:
	'architecture' ':'
		elements*=Element
;

Element:
	Component | Connector
;

Component:
	'component' type=ComponentType name=ID instances=INT?
;

Connector:
	'connector' type=ConnectorType from=[Component] '->' to=[Component]
;

ComponentType:
	'database' | 'backend' | 'frontend' | 'load_balancer'
;

ConnectorType:
	'http' | 'db_connector'
;