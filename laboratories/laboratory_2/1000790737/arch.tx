Model:
    'architecture' ':' elements*=Element
;

Element:
    Component | Connector
;

Component:
    'component' type=ComponentType name=ID
;

Connector:
    'connector' type=ConnectorType from=[Component] '->' to=[Component]
;

ComponentType:
    'frontend' | BackendType | 'database' | 'load_balancer'
;

ConnectorType:
    'http' | 'db_connector'
;

BackendType:
    'backend' ('instances' '=' instances=INT)
;

